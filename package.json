{
  "name": "sport_stats_server",
  "version": "1.0.0",
  "description": "server for sport stats app",
  "main": "index.js",
  "scripts": {
    "build": "tsc src/server.ts",
    "start": "nodemon -e ts -w ./src -x npm run dev",
    "dev": "ts-node ./src/server.ts",
    "lint": "eslint --ext .js,.ts --fix ./src/",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexlatypov94/sport_stats_server.git"
  },
  "keywords": [
    "server",
    "express",
    "node",
    "mongoDB"
  ],
  "author": "Aliaksei Latypau",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexlatypov94/sport_stats_server/issues"
  },
  "homepage": "https://github.com/alexlatypov94/sport_stats_server#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongoose": "^6.4.0",
    "swagger-ui-express": "^4.4.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2",
    "yamljs": "^0.3.0"
  }
}
